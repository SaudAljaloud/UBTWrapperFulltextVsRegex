		</dependency>
		<dependency>
			<!-- mvn deploy:deploy-file \ -DgroupId=it.unimi \ -DartifactId=fastutil 
				\ -Dversion=5.1.5 \ -Dpackaging=jar \ -DrepositoryId=bigdata.releases \ -Durl=file:repo/ 
				\ -Dfile=lib/fastutil-5.1.5.jar -->
			<groupId>it.unimi</groupId>
